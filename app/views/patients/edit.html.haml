%h1 Update Patient Demographics

.row
  .large-6.columns

    = form_for :patient, url: patient_path(@patient), :method => :put do |f|

      = render partial: "patients/demographic_form", locals: { patient: f }

      = submit_tag "Update Demographics"

= content_for :secondary_nav do
  = link_to "Back to patient list", patients_path